{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "integer"]
    },
    "iid": {
      "type": ["null", "integer"]
    },
    "project_id": {
      "type": ["null", "integer"]
    },
    "group_id": {
      "type": ["null", "integer"]
    },
    "title": {
      "type": ["null", "string"]
    },
    "description": {
      "type": ["null", "string"]
    },
    "start_date": {
      "type": ["null", "string"]
    },
    "due_date": {
      "type": ["null", "string"]
    },
    "state": {
      "type": ["null", "string"]
    },
    "created_at": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    },
    "updated_at": {
      "anyOf": [
        {
          "type": "string",
          "format": "date-time"
        },
        {
          "type": "null"
        }
      ]
    }
  }
}
